<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

"-//Hibernate/Hibernate Mapping DTD 3.0//EN"

"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.trainingdiary.DiaryBean" table="diaries">
		
		<id name="id" type="int" column="id" >
			<generator class="native"/>
		</id>

		<property name="nameOfDiary">
			<column name="nameOfDiary" />
		</property>
		
		<property name="diaryCreationDate">
			<column name="diaryCreationDate" />
		</property>
		
		<property name="diaryDescription">
			<column name="diaryDescription" />
		</property>
		
		<property name="choosedTrainingPlan">
			<column name="choosedTrainingPlan" />
		</property>
		
			
	</class>



</hibernate-mapping>