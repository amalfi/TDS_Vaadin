<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

"-//Hibernate/Hibernate Mapping DTD 3.0//EN"

"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.trainingdiary.Training" table="trainings">
		
		<id name="id" type="int" column="id" >
			<generator class="native"/>
		</id>

		<property name="description">
			<column name="description" />
		</property>
		
		<property name="choosedDiary">
			<column name="choosedDiary" />
		</property>	
		
		<property name="trainingProgramName">
			<column name="trainingProgramName" />
		</property>	
		
		<property name="numberOfExcersises">
			<column name="numberOfExcersises" />
		</property>	
		
		<property name="numberOfSets">
			<column name="numberOfSets" />
		</property>	
		
		<property name=" breakBetweenSets">
			<column name="breakBetweenSets" />
		</property>	
	
		
	</class>

</hibernate-mapping>